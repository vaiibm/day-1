
public class Student {
private String name;
private byte age;

void study() {
	System.out.println(this.name+" is studying");
}
public 

Student(){
}
Student(String name){
setName(name);	
}
public String getName() {
	return name;
}
public void setName(String name) {
	if(name == null || name.trim().equals("")) {
		throw new IllegalArgumentException("Name is madt");
	}
	this.name = name;
}
}